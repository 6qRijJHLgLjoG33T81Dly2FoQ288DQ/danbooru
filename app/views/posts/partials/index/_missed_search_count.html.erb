<script type="text/javascript">
  $(function() {
    var tags = $("#tags").val();
    if ($(".post-preview").size() === 0) {
      $.post("<%= Danbooru.config.report_server %>/missed_searches", {
        session_id: "<%= session_id %>",
        tags: tags,
        sig: "<%= sig %>"
      });
    }
  });
</script>
